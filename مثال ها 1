برنامه ای بنویسد که اطلاعات جعبه ای از قبیل طول،عرض و ارتفاع خوانده و با استفاده از متد 
حجم جعبه را محاسبه و در متد Main نمایش دهد.
using System;

namespace project 
{
    class test
    {
    public static void Main()
    {
        int L = Convert.ToInt32(Console.ReadLine());
            int H= Convert.ToInt32(Console.ReadLine());
        int W= Convert.ToInt32(Console.ReadLine());
            double show = Calc(L, H, W);
        Console.WriteLine(show);
    }//end Main
    private double Calc(int num1,int num2, int num3)
    {
        double Result = num1 * num2 * num3;
        return Result;
    }//end function
    }//end class
}//end namespace

برنامه ای بنویسید عددی از ورودی بخواند اگر عدد فرد باشد مجموع و حاصل ضرب یک تا آن عدد فرد 
را چاپ کند و اگر زوج بود نیز همین عملیات را از 2 انجام دهد.
using System;

namespace project 
{
    class test
    {
    public static void Main()
    {
            int X = Convert.ToInt32(Console.ReadLine());
            if (X % 2 == 0)
            {
                even(x);
            }
            else
            {
                odd(x);
            }
    }//end Main
    private int even(int x)
    {
            int sum = 0, zarb = 1;
            for(int i = 2; i <= x; i++)
            {
                sum += i;
                zarb *= i;
                Console.WriteLine("majmoe" + sum);
                Console.WriteLine("haselzarb" + zarb);
            }
        }//end function
    }//end class
}//end namespace

